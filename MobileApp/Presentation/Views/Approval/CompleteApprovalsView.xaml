<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodel="clr-namespace:Punjab_Ornaments.Presentation.Viewmodels.Approval"
             xmlns:model="clr-namespace:Punjab_Ornaments.Models.Stock"
             xmlns:template="clr-namespace:Punjab_Ornaments.Presentation.Views.Approval.ApprovalTemplates"
             x:DataType="viewmodel:CompleteApprovalViewModel"
             x:Class="Punjab_Ornaments.Presentation.Views.Approval.CompleteApprovalsView"
             Title="CompleteApprovalsView">
    <VerticalStackLayout>
        <CollectionView ItemsSource="{Binding PuchaseList}">
            <CollectionView.ItemTemplate>
                <DataTemplate>
                    <template:Purchasetemplate/>
                </DataTemplate>
            </CollectionView.ItemTemplate>
        </CollectionView>
    </VerticalStackLayout>
</ContentPage>